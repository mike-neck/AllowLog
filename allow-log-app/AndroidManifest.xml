<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:a="http://schemas.android.com/apk/res/android"
      package="jp.group.android.atec.allowlog"
      a:versionCode="1"
      a:versionName="1.0">
    <application a:label="@string/app_name" >
        <activity a:name="MainActivity"
                  a:label="@string/app_name"
                  a:theme="@android:style/Theme.Light.NoTitleBar.Fullscreen">
            <intent-filter>
                <action a:name="android.intent.action.MAIN" />
                <category a:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity a:name="RegisterActivity"
            a:label="@string/app_name"
            a:theme="@android:style/Theme.Light.NoTitleBar.Fullscreen"></activity>
        <activity a:name=".HistoryActivity"
            a:label="@string/app_name"
            a:theme="@android:style/Theme.Light.NoTitleBar.Fullscreen"></activity>
        <activity a:name=".ErrorDialog"
            a:label="@string/app_name"
            a:theme="@android:style/Theme.Dialog"></activity>
    </application>

</manifest>
